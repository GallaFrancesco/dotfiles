!#include "/home/francesco/.colours/solarized"

Xcursor.theme: gentoo

!Xft.autohint: true
Xft.antialias: true
!Xft.hinting: true
!Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgba
Xft.lcdfilter: lcddefault
URxvt*scrollBar:                      false
URxvt*transparent:                    true

URxvt*shading:                        8
URxvt.font: 	       		      	  xft:monospace:size=13:antialias=true,xft:Kochi:antialias=true
